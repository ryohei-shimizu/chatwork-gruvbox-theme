{
  "name": "chatwork-gruvbox",
  "version": "1.0.0",
  "description": "Gruvbox Theme for Chatwork(used Web Extension API)",
  "main": "index.js",
  "scripts": {
    "dev": "web-ext run --source-dir . --start-url https://kcw.kddi.ne.jp --keep-profile-changes",
    "sign": "web-ext sign --api-key $FIREFOX_ADDON_KEY --api-secret $FIREFOX_ADDON_SECRET_KEY",
    "build": "web-ext build --overwrite-dest && npm run sign",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "web-ext lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/k-ymmt/chatwork-gruvbox-theme.git"
  },
  "author": "k.yamamoto@fenrir.co.jp",
  "license": "MIT",
  "devDependencies": {
    "web-ext": "^3.0.0"
  }
}
